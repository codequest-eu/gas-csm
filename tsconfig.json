{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "sourceMap": false,
    "target": "ESNext",
    "lib": ["ESNext"],
    "strict": true,
    "moduleResolution": "node",
    "module": "none",
    "jsx": "react",
    "jsxFactory": "Jsx.buildComponent",
    "noEmitOnError": true,
    "types": ["google-apps-script", "node"],
    "declaration": true,
    "declarationDir": "./types",
    "outDir": "./dist"
  },
  "include": ["./src/**/*.ts"]
}
